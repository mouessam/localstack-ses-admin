{
  "name": "@ses-admin/server",
  "version": "0.1.0",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json --noEmit && node esbuild.config.js",
    "dev": "NODE_ENV=development tsx watch --env-file=../../.env src/main.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "node --test --import tsx src/**/*.test.ts",
    "test:e2e": "node --test --import tsx test/e2e/*.test.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "3.975.0",
    "@fastify/busboy": "3.2.0",
    "@fastify/static": "8.2.0",
    "@ses-admin/shared": "0.1.0",
    "fastify": "5.6.0",
    "nodemailer": "7.0.6",
    "undici": "7.20.0",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "form-data": "4.0.5",
    "tsx": "4.20.5",
    "typescript": "5.8.3"
  }
}
